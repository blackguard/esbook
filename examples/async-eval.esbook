            "input": "importScripts('lib/iterable-util.js');\nconst { range, asyncify_iterable, chain_async } = iterable_util;\n\n(async () => {\n\n    const ai = asyncify_iterable(range(10), 100);\n\n    println('flat');\n    const ai2 = asyncify_iterable(range(100, 105), 30);\n    const aix = asyncify_iterable([111, asyncify_iterable(['a', [ 'b', 'cd', ai, 'ef' ], ai2, 'ghi'], 300), 222], 200);\n    for (const depth of range(6)) {\n        print(` depth=${depth}:`);\n        await aix.flat(depth).for_each(x => print(` ${Array.isArray(x) ? `[${x}]` : x}`));\n        println();\n    }\n\n    println();\n    println('chain');\n    await chain_async(ai, 'abc', ai2).for_each(x => print(` ${Array.isArray(x) ? `[${x}]` : x}`));\n    println();\n\n    println();\n    println('tee');\n    const streams = ai.tee(3);\n    for (const i in streams) {\n        await streams[i].for_each(x => print(` ${x}/${i}`));\n        println();\n    }\n\n    println();\n    println('asyncify string');\n    await asyncify_iterable('abcdefg', 100).for_each(x => print(` ${x}`));\n    println();\n\n})();",
