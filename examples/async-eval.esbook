            "input": "importScripts('lib/iterable-util.js');\nconst { range, asyncify_iterable } = iterable_util;\n\n(async () => {\n    const ai = asyncify_iterable(range(10), 100);\n\n    println('START: map');\n    for await (const i of ai.map(x => x**2)) {\n        print(` ${i}`);\n    }\n    println();\n    println('DONE');\n\n    println();\n    println('START: map/pluck');\n    for await (const i of ai.map(x => ({ value: ` | x is ${x} |` })).pluck('value')) {\n        print(` ${i}`);\n    }\n    println();\n    println('DONE');\n\n    println();\n    println('START: for_each');\n    await ai.for_each(x => print(` ${x}`));\n    println();\n    println('DONE');\n\n    println();\n    println('START: cycle');\n    for await (const i of ai.cycle(3)) {\n        print(` ${i}`);\n    }\n    println();\n    println('DONE');\n\n    println();\n    println('START: nth');\n    println(await ai.nth(7));\n    println('DONE');\n\n    println();\n    println(`some  x <  5: ${await ai.some(x => x < 5)}`);\n    println(`some  x >  8: ${await ai.some(x => x > 8)}`);\n    println(`some  x >  9: ${await ai.some(x => x > 9)}`);\n    println(`every x <  5: ${await ai.every(x => x < 5)}`);\n    println(`every x >= 0: ${await ai.every(x => x >= 0)}`);\n\n    println();\n    println('map/find');\n    printf('%4j\\n', await ai.map((x, i) => ({ square: x**2, x, i })).find(y => y.square > 5));\n\n})();",
